from collections import defaultdict

d = defaultdict(list)
d['x'] = [1,2,3,4]
d['y'] = [5,6,7,8]
# d is {'y': [5, 6, 7, 8], 'x': [1, 2, 3, 4]}

d['z'].append(10)
d['x'].append(9)
print d
# d is now {'y': [5, 6, 7, 8], 'x': [1, 2, 3, 4, 9], 'z': [10]}


